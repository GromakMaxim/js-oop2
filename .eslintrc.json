{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style":"off",
        "no-plusplus":"off",
        "no-restricted-syntax":"off",
        "no-undef":"warn"
    }
}